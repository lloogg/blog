<%- date(page.date, 'YYYY/MM/DD' ) %> tag: <% page.tags.forEach(tag=> { %>
<a
  href="<%-
tag.name %>"
  ><%- tag.name %></a
>
<% }) %> <%- page.content %>
<div style="display: flex; justify-content: space-around">
  <% if (page.prev) { %>
  <a href="<%- url_for(page.prev.path) %> "> <%- page.prev.title %> </a>
  <% } %> <% if (page.next) { %>
  <a href="<%- url_for(page.next.path) %> "> <%- page.next.title %> </a>
  <% } %>
</div>
